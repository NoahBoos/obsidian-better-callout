{
  "name": "obsidian-better-callout",
  "version": "1.0.0",
  "description": "Better Callouts is a package of CSS Snippets that brings new pre-built callouts, as well as new metadata to edit callouts easily.",
  "scripts": {
    "build-callout-metadata": "sass --watch --no-source-map --no-charset src/callout-metadata.scss:\"build/Custom Callout - Metadata.css\"",
    "build-callout-templates": "sass --watch --no-source-map --no-charset src/callout-templates.scss:\"build/Custom Callout - Templates.css\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoahBoos/obsidian-better-callout.git"
  },
  "author": "Noah Boos AKA Rift",
  "license": "Rift Open Source Software License v3.0",
  "bugs": {
    "url": "https://github.com/NoahBoos/obsidian-better-callout/issues"
  },
  "homepage": "https://github.com/NoahBoos/obsidian-better-callout#readme",
  "dependencies": {
    "sass": "^1.86.3"
  }
}
